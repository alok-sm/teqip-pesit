<br><br><br>
   
<center><h1>Listing partners</h1></center><br>
<center><% if user_signed_in? %>
<%= link_to 'New Partner', new_partner_path %>
<% end %><center>
<center><h4>List of projects under industry partners</h4></center>

<% @partners.each do |partner| %>
   <a href="/partners/<%= partner.id %>">  
  <div class="card">
	<div class="card-inner">
	<center><h3>Name :&nbsp<%= partner.name %></h3></center>
	<center><h4>Industry Or Academic? :&nbsp<%= partner.partner_type %></h4></center>
	<center><h4>List Of Activities :&nbsp<%= partner.activities %></h4></center> 
  </div>
</div>
</a>
<center><%= paginate @partners %></center>
<center>
    <% if user_signed_in? %>
    <%= link_to 'Edit', edit_partner_path(partner) %> |
    <%= link_to 'Destroy', partner, method: :delete, data: { confirm: 'Are you sure?' } %>
    <% end %>
  </center>
<% end %>
<br><br>
<center><h4>List of projects under academic partners</h4></center>


